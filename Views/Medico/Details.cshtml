@model WebClinica.Models.Medico

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Content.cshtml";
}
<head>
    <title>Detalles medicos</title>
</head>
<partial name="_Menu" />
<div class="container contenedorPrincipalCreate">
    <main class="pb-3" role="main">
        <div class="row">
            <form asp-action="Details" method="post">
                <div class="container-fluid">
                    <fieldset>
                        <legend class="text-center header">Detalles medico</legend>
                        <br class="brDivisor" />
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-group row">
                            <div class="col-md-3 text-align-right">
                                <label asp-for="MedicoId" class="control-label"></label>
                            </div>
                            <div class="col-md-6">
                                <input asp-for="MedicoId" disabled class="form-control" />
                            </div>
                            <span asp-validation-for="MedicoId" class="text-danger"></span>
                        </div>
                        <div class="form-group row">
                            <div class="col-md-3 text-align-right">
                                <label asp-for="Nombre" class="control-label"></label>
                            </div>
                            <div class="col-md-6">
                                <input asp-for="Nombre" disabled class="form-control"  />
                            </div>
                            <span asp-validation-for="Nombre" class="text-danger"></span>
                        </div>
                        <div class="form-group row">
                            <div class="col-md-3 text-align-right">
                                <label asp-for="Apellidos" class="control-label"></label>
                            </div>
                            <div class="col-md-6">
                                <input asp-for="Apellidos" disabled class="form-control" />
                            </div>
                            <span asp-validation-for="Apellidos" class="text-danger"></span>
                        </div>
                        <div class="form-group row">
                            <div class="col-md-3 text-align-right">
                                <label asp-for="Direccion" class="control-label"></label>
                            </div>
                            <div class="col-md-6">
                                <input asp-for="Direccion" disabled class="form-control" />
                            </div>
                            <span asp-validation-for="Direccion" class="text-danger"></span>
                        </div>
                        <div class="form-group row">
                            <div class="col-md-3 text-align-right">
                                <label asp-for="TelefonoFijo" class="control-label"></label>
                            </div>
                            <div class="col-md-6">
                                <input asp-for="TelefonoFijo" disabled class="form-control"/>
                            </div>
                            <span asp-validation-for="TelefonoFijo" class="text-danger"></span>
                        </div>
                        <div class="form-group row">
                            <div class="col-md-3 text-align-right">
                                <label asp-for="TelefonoCelular" class="control-label"></label>
                            </div>
                            <div class="col-md-6">
                                <input asp-for="TelefonoCelular" disabled class="form-control" />
                            </div>
                            <span asp-validation-for="TelefonoCelular" class="text-danger"></span>
                        </div>
                        <div class="form-group row">
                            <div class="col-md-3 text-align-right">
                                <label asp-for="EspecialidadId" class="control-label"></label>
                            </div>
                            <div class="col-md-6">
                                <input asp-for="EspecialidadId" disabled class="form-control"/>
                            </div>
                            <span asp-validation-for="EspecialidadId" class="text-danger"></span>
                        </div>
                    </fieldset>
                    <div class="row contenedorBotonesCreate">
                        <div class="col-md-4">
                            <a class="btn btn-info btn-lg botonesRegresar" id="regresar" asp-action="Index">Regresar</a>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </main>
</div>
<partial name="_EndPag" />