
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Content.cshtml";
    List<SelectListItem> Lista = ViewBag.ListaTipoUsuario;
}

<div class="limiter">
    <div class="container-login100">
        <div class="wrap-login100 p-l-85 p-r-85 p-t-55 p-b-55">
            <form class="login100-form validate-form flex-sb flex-w" asp-controller="SingIn" method="post" id="viewLogin">
                <span class="login100-form-title p-b-32">
                    Registro de usuario
                </span>
                <span class="txt1 p-b-11">
                    Ingrese la cedula
                </span>
                <div class="wrap-input100 validate-input m-b-36">
                    <input class="input100" type="text" id="Cedula" value="" required />
                    <span class="focus-input100"></span>
                </div>
                <span class="txt1 p-b-11">
                    Ingrese su nombre de usuario
                </span>
                <div class="wrap-input100 validate-input m-b-36">
                    <input class="input100" type="text" id="name" value="" required />
                    <span class="focus-input100"></span>
                </div>
                <span class="txt1 p-b-11">
                    Ingrese su Contraseña
                </span>
                <div class="wrap-input100 validate-input m-b-12">
                    <span class="btn-show-pass">
                        <i class="fa fa-eye"></i>
                    </span>
                    <input class="input100" onkeyup="KeyUpEnterLogin()" type="password" id="password" value="" required />
                    <span class="focus-input100"></span>
                </div>
                <div class="container-login100-form-btn">
                    <a class="login100-form-btn" id="Ingresar" onclick="SingIn()">
                        Registrarse
                    </a>
                </div>
            </form>
        </div>
    </div>
</div>

